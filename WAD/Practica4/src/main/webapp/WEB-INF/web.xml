<?xml version="1.0"?>
<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>Filtro</filter-name>
		<filter-class>mx.ipn.escom.wad.filters.Filtro</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Filtro</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>FiltroRecursos</filter-name>
		<filter-class>mx.ipn.escom.wad.filters.FiltroRecursos</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>FiltroRecursos</filter-name>
		<url-pattern>/Privado</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>mx.ipn.escom.wad.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Privado</servlet-name>
		<servlet-class>mx.ipn.escom.wad.servlets.Privado</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Publico</servlet-name>
		<servlet-class>mx.ipn.escom.wad.servlets.Publico</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet1</servlet-name>
		<servlet-class>mx.ipn.escom.wad.servlets.Servlet1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SessionServlet</servlet-name>
		<servlet-class>mx.ipn.escom.wad.servlets.SessionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Privado</servlet-name>
		<url-pattern>/Privado</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Publico</servlet-name>
		<url-pattern>/Publico</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet1</servlet-name>
		<url-pattern>/Servlet1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SessionServlet</servlet-name>
		<url-pattern>/SessionServlet</url-pattern>
	</servlet-mapping>
</web-app>
